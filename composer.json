{
    "name": "spatie/laravel-remote",
    "description": "Execute Artisan commands on remote servers",
    "keywords": [
        "spatie",
        "laravel-remote"
    ],
    "homepage": "https://github.com/spatie/laravel-remote",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-package-tools": "^1.9",
        "spatie/ssh": "^1.5"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.9",
        "spatie/ray": "^1.21",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Remote\\": "src",
            "Spatie\\Remote\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Remote\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Remote\\RemoteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
